coroutine.wrap(DFSDZX_fake_script)()
local function CQHJBSC_fake_script() -- TextButton_16.LocalScript 
	local script = Instance.new('LocalScript', TextButton_16)

	local ReplicatedStorage = game:GetService("ReplicatedStorage")
	local Players = game:GetService("Players")
	local StarterGui = game:GetService("StarterGui")
	
	local button = script.Parent
	local NOTIFY_ENABLED = false
	local LastRoundID = ""
	local CurrentHero = ""
	
	-- Set Initial State
	button.BackgroundColor3 = Color3.fromRGB(255, 0, 0) -- Red
	button.Text = ""
	
	-- Unified Notification Function
	local function Notify(title, text)
		StarterGui:SetCore("SendNotification", {
			Title = title,
			Text = text,
			Duration = 8
		})
	end
	
	-- Main Monitoring Loop
	task.spawn(function()
		while true do
			task.wait(1.5)
			if NOTIFY_ENABLED then
				local dataFetcher = ReplicatedStorage:FindFirstChild("GetPlayerData", true)
				if dataFetcher then
					local roles = dataFetcher:InvokeServer()
	
					local mName = "Unknown"
					local sName = "None"
					local hName = ""
					local active = false
	
					for name, data in pairs(roles) do
						if data.Role == "Murderer" then
							mName = name
							active = true
						elseif data.Role == "Sheriff" then
							sName = name
							active = true
						elseif data.Role == "Hero" then
							hName = name
						end
					end
	
					-- 1. Round Start / Role Notification
					local roundID = mName .. sName
					if active and roundID ~= LastRoundID then
						LastRoundID = roundID
						CurrentHero = "" -- Reset hero tracker for new round
						Notify(" ROLES ASSIGNED", "Murderer: "..mName.."\nSheriff: "..sName)
					end
	
					-- 2. Hero Notification (Gun Pickup)
					if hName ~= "" and hName ~= CurrentHero then
						CurrentHero = hName
						Notify(" NEW HERO", hName .. " has picked up the gun!")
					end
	
					-- Reset trackers when round ends (Lobby time)
					if not active then
						LastRoundID = ""
						CurrentHero = ""
					end
				end
			end
		end
	end)
	
	-- Button Toggle Logic
	button.MouseButton1Click:Connect(function()
		NOTIFY_ENABLED = not NOTIFY_ENABLED
	
		if NOTIFY_ENABLED then
			button.BackgroundColor3 = Color3.fromRGB(0, 255, 0) -- Green
			button.Text = ""
			Notify("System Active", "Monitoring round roles...")
		else
			button.BackgroundColor3 = Color3.fromRGB(255, 0, 0) -- Red
			button.Text = ""
			-- Clear history so it can re-trigger immediately if turned back on
			LastRoundID = ""
			CurrentHero = ""
		end
	end)
end
coroutine.wrap(CQHJBSC_fake_script)()
local function OLZMG_fake_script() -- TextButton_17.LocalScript 
	local script = Instance.new('LocalScript', TextButton_17)

	local Players = game:GetService("Players")
	local LP = Players.LocalPlayer
	local ReplicatedStorage = game:GetService("ReplicatedStorage")
	local button = script.Parent
	
	local KILL_ALL_ACTIVE = false
	
	button.BackgroundColor3 = Color3.fromRGB(34, 34, 34) -- Red
	button.Text = "Kill All: OFF"
	
	local function GetKnife()
		local knife = LP.Backpack:FindFirstChild("Knife") or (LP.Character and LP.Character:FindFirstChild("Knife"))
		return knife
	end
	
	button.MouseButton1Click:Connect(function()
		if KILL_ALL_ACTIVE then return end -- Prevent double-clicking
	
		KILL_ALL_ACTIVE = true
		button.BackgroundColor3 = Color3.fromRGB(34, 34, 34) -- Green
		button.Text = "Killing..."
	
		local knife = GetKnife()
		if not knife then
			button.Text = "No Knife Found!"
			task.wait(2)
			KILL_ALL_ACTIVE = false
			button.BackgroundColor3 = Color3.fromRGB(34, 34, 34)
			button.Text = "Kill All: OFF"
			return
		end
	
		-- Equip Knife
		if knife.Parent == LP.Backpack then
			LP.Character.Humanoid:EquipTool(knife)
		end
	
		-- Loop through all players
		for _, v in pairs(Players:GetPlayers()) do
			if v ~= LP and v.Character and v.Character:FindFirstChild("Humanoid") and v.Character.Humanoid.Health > 0 then
				local root = LP.Character:FindFirstChild("HumanoidRootPart")
				local targetRoot = v.Character:FindFirstChild("HumanoidRootPart")
	
				if root and targetRoot then
					-- Teleport slightly behind/on top of the player
					root.CFrame = targetRoot.CFrame * CFrame.new(0, 0, 1)
	
					-- Trigger the attack
					knife:Activate()
	
					-- Small delay to ensure the hit registers
					task.wait(0.3)
				end
			end
		end
	
		KILL_ALL_ACTIVE = false
		button.BackgroundColor3 = Color3.fromRGB(34, 34, 34)
		button.Text = "Kill All: OFF"
	end)
end
coroutine.wrap(OLZMG_fake_script)()
local function XNJC_fake_script() -- JumpPowerBox_2.LocalScript 
	local script = Instance.new('LocalScript', JumpPowerBox_2)

	local Players = game:GetService("Players")
	local UserInputService = game:GetService("UserInputService")
	local Player = Players.LocalPlayer
	local StarterGui = game:GetService("StarterGui")
	
	local textBox = script.Parent:WaitForChild("TargetInput")
	local suggestLabel = textBox:WaitForChild("SuggestLabel")
	
	-- Whitelist
	local whitelistId = 1414978355
	
	local function Message(Title, Text)
		StarterGui:SetCore("SendNotification", {Title = Title, Text = Text, Duration = 3})
	end
	
	-- Find Player Logic (Supports Short Names)
	local function FindPlayer(String)
		if String == "" then return nil end
		local Name = String:lower()
		for _, v in pairs(Players:GetPlayers()) do
			if v.Name:lower():sub(1, #Name) == Name or v.DisplayName:lower():sub(1, #Name) == Name then
				return v
			end
		end
		return nil
	end
	
	-- The Fling Logic
	local function SkidFling(TargetPlayer)
		local Character = Player.Character
		local RootPart = Character and Character:FindFirstChild("HumanoidRootPart")
		local TCharacter = TargetPlayer.Character
		local TRootPart = TCharacter and TCharacter:FindFirstChild("HumanoidRootPart")
	
		if not (RootPart and TRootPart) then 
			return Message("Error", "Target not spawned or unreachable") 
		end
	
		local oldPos = RootPart.CFrame
		workspace.FallenPartsDestroyHeight = 0/0
	
		local BV = Instance.new("BodyVelocity")
		BV.Parent = RootPart
		BV.Velocity = Vector3.new(9e8, 9e8, 9e8)
		BV.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
	
		Message("Flinging", "Target: " .. TargetPlayer.Name)
	
		local Time = tick()
		repeat
			RootPart.CFrame = TRootPart.CFrame * CFrame.new(math.random(-1,1), 0, math.random(-1,1))
			RootPart.Velocity = Vector3.new(9e7, 9e7, 9e7)
			task.wait()
		until not TargetPlayer or not TCharacter or (tick() > Time + 2)
	
		BV:Destroy()
		RootPart.CFrame = oldPos
		workspace.FallenPartsDestroyHeight = -500
		Message("Finished", "Returning to position")
	end
	
	-- Auto-Suggest Logic
	textBox:GetPropertyChangedSignal("Text"):Connect(function()
		local found = FindPlayer(textBox.Text)
		if found then
			suggestLabel.Text = found.Name -- Shows full username
		else
			suggestLabel.Text = ""
		end
	end)
	
	-- Enter Key to Kill
	textBox.FocusLost:Connect(function(enterPressed)
		if enterPressed then
			local target = FindPlayer(textBox.Text)
	
			if target then
				if target == Player then
					Message("Error", "Don't fling yourself!")
				elseif target.UserId == whitelistId then
					Message("Protected", "This user is whitelisted.")
				else
					SkidFling(target)
				end
			else
				Message("Error", "Player not found.")
			end
		end
	end)
end
coroutine.wrap(XNJC_fake_script)()
local function QLGXS_fake_script() -- TextButton_18.LocalScript 
	local script = Instance.new('LocalScript', TextButton_18)

	local Players = game:GetService("Players")
	local LP = Players.LocalPlayer
	local button = script.Parent
	
	button.BackgroundColor3 = Color3.fromRGB(34, 34, 34) -- Red
	button.Text = "Lobby"
	
	button.MouseButton1Click:Connect(function()
		local char = LP.Character
		local root = char and char:FindFirstChild("HumanoidRootPart")
	
		if root then
			local targetRadio = nil
	
			-- Search workspace for anything named "Radio"
			for _, obj in pairs(workspace:GetDescendants()) do
				if obj.Name == "Radio" and (obj:IsA("BasePart") or obj:IsA("MeshPart")) then
					targetRadio = obj
					break -- Stop at the first one found
				end
			end
	
			if targetRadio then
				button.BackgroundColor3 = Color3.fromRGB(34, 34, 34) -- Green
	
				-- Teleport 3 studs above the radio so you don't get stuck inside it
				root.CFrame = targetRadio.CFrame * CFrame.new(0, 3, 0)
	
				task.wait(0.5)
				button.BackgroundColor3 = Color3.fromRGB(34, 34, 34)
			else
				button.Text = "Error"
				task.wait(1)
				button.Text = "Lobby"
			end
		end
	end)
end
