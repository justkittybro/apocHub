-- Scripts:

local function CCOIGH_fake_script() -- TheAllPanel.LocalScript 
	local script = Instance.new('LocalScript', TheAllPanel)

	-- LocalScript inside your ScreenGui
	local UIS = game:GetService("UserInputService")
	local GUI = script.Parent
	local isOpen = true -- starts visible
	
	-- Optional: initially hide all children if you want
	-- for _, child in pairs(GUI:GetChildren()) do
	--     if child:IsA("GuiObject") then
	--         child.Visible = false
	--     end
	-- end
	-- isOpen = false
	
	UIS.InputBegan:Connect(function(input, gameProcessed)
		if gameProcessed then return end
		if input.KeyCode == Enum.KeyCode.K then
			isOpen = not isOpen
			for _, child in pairs(GUI:GetChildren()) do
				if child:IsA("GuiObject") then
					child.Visible = isOpen
				end
			end
		end
	end)
	
end
coroutine.wrap(CCOIGH_fake_script)()
local function BGAGC_fake_script() -- TheAllPanel.LocalScript 
	local script = Instance.new('LocalScript', TheAllPanel)

	local screenGui = script.Parent
	
	-- DisplayOrder determines the layering of ScreenGuis.
	-- 999999999 is usually high enough to beat any standard Roblox UI.
	screenGui.DisplayOrder = 999999999
	
	-- Ignores the top bar inset (makes UI fill the very top of the screen)
	screenGui.IgnoreGuiInset = true
	
	-- Optional: Re-enforces the order if other scripts try to change it
	screenGui:GetPropertyChangedSignal("DisplayOrder"):Connect(function()
		if screenGui.DisplayOrder ~= 999999999 then
			screenGui.DisplayOrder = 999999999
		end
	end)
	
	print("UI Layering set to maximum priority.")
end
coroutine.wrap(BGAGC_fake_script)()
local function RAXEH_fake_script() -- TheAllPanel.LocalScript 
	local script = Instance.new('LocalScript', TheAllPanel)

	local StarterGui = game:GetService("StarterGui")
	
	-- Function to send the notification
	local function Notify()
		-- We use a pcall to prevent errors if the CoreGui hasn't loaded yet
		local success, err = pcall(function()
			StarterGui:SetCore("SendNotification", {
				Title = "Eclipse Hub Loaded!",
				Text = "Eclipse hub Old UI has been loaded",
				Icon = "rbxassetid://6023454774", -- Optional: A cool info icon
				Duration = 10000000
			})
		end)
	
		-- Retry if the game system isn't ready yet
		if not success then
			task.wait(1)
			Notify()
		end
	end
	
	-- Run the notification
	Notify()
end
coroutine.wrap(RAXEH_fake_script)()
local function MLAJ_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	-- SERVICES
	local ReplicatedStorage = game:GetService("ReplicatedStorage")
	local Players = game:GetService("Players")
	local RunService = game:GetService("RunService")
	
	-- PLAYER
	local LP = Players.LocalPlayer
	local button = script.Parent
	
	-- STATE
	local ESP_ENABLED = false
	local connection
	local roles
	local Murder, Sheriff, Hero
	
	-- BUTTON DEFAULT (OFF)
	button.BackgroundColor3 = Color3.fromRGB(255, 0, 0) -- Red
	button.Text = ""
	
	-- FUNCTIONS
	local function IsAlive(player)
		if not roles then return false end
		for name, data in pairs(roles) do
			if player.Name == name then
				return not data.Killed and not data.Dead
			end
		end
		return false
	end
	
	local function CreateOverhead(plr)
		if not plr.Character or not plr.Character:FindFirstChild("Head") then return end
		if plr.Character:FindFirstChild("ESP_Tag") then return end
	
		local billboard = Instance.new("BillboardGui")
		billboard.Name = "ESP_Tag"
		billboard.Size = UDim2.new(0, 200, 0, 50)
		billboard.Adornee = plr.Character.Head
		billboard.AlwaysOnTop = true
		billboard.ExtentsOffset = Vector3.new(0, 3, 0)
		billboard.Parent = plr.Character
	
		local label = Instance.new("TextLabel")
		label.Size = UDim2.new(1, 0, 1, 0)
		label.BackgroundTransparency = 1
		label.TextStrokeTransparency = 0
		label.TextScaled = false
		label.TextSize = 14
		label.Font = Enum.Font.SourceSansBold
		label.TextColor3 = Color3.new(1, 1, 1)
		label.Parent = billboard
	end
	
	local function UpdateESP()
		for _, plr in pairs(Players:GetPlayers()) do
			if plr ~= LP and plr.Character and plr.Character:FindFirstChild("Head") then
				CreateOverhead(plr)
				local tag = plr.Character:FindFirstChild("ESP_Tag")
				if tag and tag:FindFirstChild("TextLabel") then
					local label = tag.TextLabel
					local roleName = "Innocent"
					local roleColor = Color3.fromRGB(0, 255, 0) -- Green
	
					if plr.Name == Sheriff and IsAlive(plr) then
						roleName = "Sheriff"
						roleColor = Color3.fromRGB(0, 150, 255) -- Blue
					elseif plr.Name == Murder and IsAlive(plr) then
						roleName = "Murderer"
						roleColor = Color3.fromRGB(255, 0, 0) -- Red
					elseif plr.Name == Hero and IsAlive(plr) then
						roleName = "Hero"
						roleColor = Color3.fromRGB(255, 255, 0) -- Yellow
					end
	
					label.Text = string.format("%s\n[%s]", plr.Name, roleName)
					label.TextColor3 = roleColor
				end
			end
		end
	end
	
	local function ClearESP()
		for _, plr in pairs(Players:GetPlayers()) do
			if plr.Character and plr.Character:FindFirstChild("ESP_Tag") then
				plr.Character.ESP_Tag:Destroy()
			end
		end
	end
	
	-- TOGGLE BUTTON
	button.MouseButton1Click:Connect(function()
		ESP_ENABLED = not ESP_ENABLED
	
		if ESP_ENABLED then
			button.BackgroundColor3 = Color3.fromRGB(0, 255, 0) -- Green
	
			connection = RunService.RenderStepped:Connect(function()
				local dataFetcher = ReplicatedStorage:FindFirstChild("GetPlayerData", true)
				if dataFetcher then
					roles = dataFetcher:InvokeServer()
					for name, data in pairs(roles) do
						if data.Role == "Murderer" then Murder = name
						elseif data.Role == "Sheriff" then Sheriff = name
						elseif data.Role == "Hero" then Hero = name end
					end
					UpdateESP()
				end
			end)
		else
			button.BackgroundColor3 = Color3.fromRGB(255, 0, 0) -- Red
			if connection then
				connection:Disconnect()
				connection = nil
			end
			ClearESP()
		end
	end)
end
coroutine.wrap(MLAJ_fake_script)()
local function WDAM_fake_script() -- TextButton_2.LocalScript 
	local script = Instance.new('LocalScript', TextButton_2)

	local ReplicatedStorage = game:GetService("ReplicatedStorage")
	local Players = game:GetService("Players")
	local RunService = game:GetService("RunService")
	
	local LP = Players.LocalPlayer
	local button = script.Parent
	local ESP_ENABLED = false
	local connection
	
	-- INITIAL STATE (Starts Off)
	button.BackgroundColor3 = Color3.fromRGB(255, 0, 0) -- Red
	if button:IsA("TextButton") then
		button.Text = ""
	end
	
	local function CreateOverhead(plr)
		if not plr.Character or not plr.Character:FindFirstChild("Head") then return end
		if plr.Character:FindFirstChild("Murderer_ESP") then return end
	
		local billboard = Instance.new("BillboardGui")
		billboard.Name = "Murderer_ESP"
		billboard.Size = UDim2.new(0, 200, 0, 50)
		billboard.Adornee = plr.Character.Head
		billboard.AlwaysOnTop = true
		billboard.ExtentsOffset = Vector3.new(0, 3, 0)
		billboard.Parent = plr.Character
	
		local label = Instance.new("TextLabel")
		label.Size = UDim2.new(1, 0, 1, 0)
		label.BackgroundTransparency = 1
		label.TextStrokeTransparency = 0
		label.Font = Enum.Font.SourceSansBold
		label.TextSize = 16
		label.TextColor3 = Color3.fromRGB(255, 0, 0) -- Red
		label.Parent = billboard
		return label
	end
	
	button.MouseButton1Click:Connect(function()
		ESP_ENABLED = not ESP_ENABLED
	
		-- Toggle Color and Text
		button.BackgroundColor3 = ESP_ENABLED and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(255, 0, 0)
		if button:IsA("TextButton") then
			button.Text = ESP_ENABLED and "" or ""
		end
	
		if ESP_ENABLED then
			connection = RunService.RenderStepped:Connect(function()
				local dataFetcher = ReplicatedStorage:FindFirstChild("GetPlayerData", true)
				if dataFetcher then
					local roles = dataFetcher:InvokeServer()
					for name, data in pairs(roles) do
						if data.Role == "Murderer" and not data.Killed and not data.Dead then
							local target = Players:FindFirstChild(name)
							if target and target.Character then
								local label = CreateOverhead(target) or (target.Character:FindFirstChild("Murderer_ESP") and target.Character.Murderer_ESP.TextLabel)
								if label then label.Text = "MURDERER\n" .. target.Name end
							end
						end
					end
				end
			end)
		else
			if connection then connection:Disconnect() end
			for _, plr in pairs(Players:GetPlayers()) do
				if plr.Character and plr.Character:FindFirstChild("Murderer_ESP") then
					plr.Character.Murderer_ESP:Destroy()
				end
			end
		end
	end)
end
coroutine.wrap(WDAM_fake_script)()
local function JEZG_fake_script() -- TextButton_3.LocalScript 
	local script = Instance.new('LocalScript', TextButton_3)

	local ReplicatedStorage = game:GetService("ReplicatedStorage")
	local Players = game:GetService("Players")
	local RunService = game:GetService("RunService")
	
	local LP = Players.LocalPlayer
	local button = script.Parent
	local ESP_ENABLED = false
	local connection
	
	-- INITIAL STATE (Starts Off)
	button.BackgroundColor3 = Color3.fromRGB(255, 0, 0) -- Red
	if button:IsA("TextButton") then
		button.Text = ""
	end
	
	local function CreateOverhead(plr)
		if not plr.Character or not plr.Character:FindFirstChild("Head") then return end
		if plr.Character:FindFirstChild("Sheriff_ESP") then return end
	
		local billboard = Instance.new("BillboardGui")
		billboard.Name = "Sheriff_ESP"
		billboard.Size = UDim2.new(0, 200, 0, 50)
		billboard.Adornee = plr.Character.Head
		billboard.AlwaysOnTop = true
		billboard.ExtentsOffset = Vector3.new(0, 3, 0)
		billboard.Parent = plr.Character
	
		local label = Instance.new("TextLabel")
		label.Size = UDim2.new(1, 0, 1, 0)
		label.BackgroundTransparency = 1
		label.TextStrokeTransparency = 0
		label.Font = Enum.Font.SourceSansBold
		label.TextSize = 16
		label.TextColor3 = Color3.fromRGB(0, 170, 255) -- Blue
		label.Parent = billboard
		return label
	end
	
	button.MouseButton1Click:Connect(function()
		ESP_ENABLED = not ESP_ENABLED
	
		-- Toggle Color and Text
		button.BackgroundColor3 = ESP_ENABLED and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(255, 0, 0)
		if button:IsA("TextButton") then
			button.Text = ESP_ENABLED and "" or ""
		end
	
		if ESP_ENABLED then
			connection = RunService.RenderStepped:Connect(function()
				local dataFetcher = ReplicatedStorage:FindFirstChild("GetPlayerData", true)
				if dataFetcher then
					local roles = dataFetcher:InvokeServer()
					for name, data in pairs(roles) do
						if (data.Role == "Sheriff" or data.Role == "Hero") and not data.Killed and not data.Dead then
							local target = Players:FindFirstChild(name)
							if target and target.Character then
								local label = CreateOverhead(target) or (target.Character:FindFirstChild("Sheriff_ESP") and target.Character.Sheriff_ESP.TextLabel)
								if label then 
									label.Text = data.Role:upper() .. "\n" .. target.Name 
								end
							end
						end
					end
				end
			end)
		else
			if connection then connection:Disconnect() end
			for _, plr in pairs(Players:GetPlayers()) do
				if plr.Character and plr.Character:FindFirstChild("Sheriff_ESP") then
					plr.Character.Sheriff_ESP:Destroy()
				end
			end
		end
	end)
end
coroutine.wrap(JEZG_fake_script)()
local function VNKV_fake_script() -- Everyone.LocalScript 
	local script = Instance.new('LocalScript', Everyone)

	-- SERVICES
	local ReplicatedStorage = game:GetService("ReplicatedStorage")
	local Players = game:GetService("Players")
	local RunService = game:GetService("RunService")
	
	-- PLAYER
	local LP = Players.LocalPlayer
	local button = script.Parent
	
	-- STATE
	local ESP_ENABLED = false
	local connection
	local roles
	
	-- BUTTON DEFAULT (OFF)
	button.BackgroundColor3 = Color3.fromRGB(255, 0, 0) -- Red
	button.Text = ""
	
	-- FUNCTIONS
	local function IsAlive(player)
		for name, data in pairs(roles) do
			if player.Name == name then
				return not data.Killed and not data.Dead
			end
		end
		return false
	end
	
	local function CreateHighlight()
		for _, plr in pairs(Players:GetPlayers()) do
			if plr ~= LP and plr.Character and not plr.Character:FindFirstChild("Highlight") then
				Instance.new("Highlight", plr.Character)
			end
		end
	end
	
	local function UpdateHighlights()
		for _, plr in pairs(Players:GetPlayers()) do
			if plr ~= LP and plr.Character then
				local highlight = plr.Character:FindFirstChild("Highlight")
				if highlight then
					if plr.Name == Sheriff and IsAlive(plr) then
						highlight.FillColor = Color3.fromRGB(0, 0, 255) -- Blue
					elseif plr.Name == Murder and IsAlive(plr) then
						highlight.FillColor = Color3.fromRGB(255, 0, 0) -- Red
					elseif plr.Name == Hero and IsAlive(plr) and not IsAlive(Players:FindFirstChild(Sheriff)) then
						highlight.FillColor = Color3.fromRGB(255, 255, 0) -- Yellow
					else
						highlight.FillColor = Color3.fromRGB(0, 255, 0) -- Green
					end
				end
			end
		end
	end
	
	local function ClearHighlights()
		for _, plr in pairs(Players:GetPlayers()) do
			if plr.Character and plr.Character:FindFirstChild("Highlight") then
				plr.Character.Highlight:Destroy()
			end
		end
	end
	
	-- TOGGLE BUTTON
	button.MouseButton1Click:Connect(function()
		ESP_ENABLED = not ESP_ENABLED
	
		if ESP_ENABLED then
			button.BackgroundColor3 = Color3.fromRGB(0, 255, 0) -- Green
			button.Text = ""
	
			connection = RunService.RenderStepped:Connect(function()
				roles = ReplicatedStorage:FindFirstChild("GetPlayerData", true):InvokeServer()
	
				for name, data in pairs(roles) do
					if data.Role == "Murderer" then
						Murder = name
					elseif data.Role == "Sheriff" then
						Sheriff = name
					elseif data.Role == "Hero" then
						Hero = name
					end
				end
	
				CreateHighlight()
				UpdateHighlights()
			end)
		else
			button.BackgroundColor3 = Color3.fromRGB(255, 0, 0) -- Red
			button.Text = ""
	
			if connection then
				connection:Disconnect()
				connection = nil
			end
	
			ClearHighlights()
		end
	end)
	
end
coroutine.wrap(VNKV_fake_script)()
local function XCRF_fake_script() -- Murderer.LocalScript 
	local script = Instance.new('LocalScript', Murderer)

	-- SERVICES
	local Players = game:GetService("Players")
	local ReplicatedStorage = game:GetService("ReplicatedStorage")
	local RunService = game:GetService("RunService")
	
	local LP = Players.LocalPlayer
	local Button = script.Parent
	
	-- STATE
	local Enabled = false
	Button.BackgroundColor3 = Color3.fromRGB(255, 0, 0) -- RED OFF
	
	local function ClearHighlights()
		for _, plr in pairs(Players:GetPlayers()) do
			if plr ~= LP and plr.Character and plr.Character:FindFirstChild("Highlight") then
				plr.Character.Highlight:Destroy()
			end
		end
	end
	
	local function Update()
		if not Enabled then
			ClearHighlights()
			return
		end
	
		local roles = ReplicatedStorage:FindFirstChild("GetPlayerData", true):InvokeServer()
	
		for name, data in pairs(roles) do
			if data.Role == "Murderer" then
				local plr = Players:FindFirstChild(name)
				if plr and plr.Character then
					if not plr.Character:FindFirstChild("Highlight") then
						local h = Instance.new("Highlight")
						h.FillColor = Color3.fromRGB(255, 0, 0)
						h.OutlineColor = Color3.fromRGB(255, 0, 0)
						h.Parent = plr.Character
					end
				end
			end
		end
	end
	
	Button.MouseButton1Click:Connect(function()
		Enabled = not Enabled
		Button.BackgroundColor3 = Enabled and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(255, 0, 0)
	end)
	
	RunService.RenderStepped:Connect(Update)
	
end
coroutine.wrap(XCRF_fake_script)()
