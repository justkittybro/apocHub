coroutine.wrap(MUYUGQ_fake_script)()
local function KUUVEK_fake_script() -- TextButton_4.LocalScript 
	local script = Instance.new('LocalScript', TextButton_4)

	local player = game.Players.LocalPlayer
	local button = script.Parent
	local UIS = game:GetService("UserInputService")
	local RunService = game:GetService("RunService")
	
	local flying = false
	local speed = 50
	
	-- COLORS
	local OFF_COLOR = Color3.fromRGB(255, 0, 0) -- red
	local ON_COLOR  = Color3.fromRGB(60, 255, 60) -- green
	
	button.BackgroundColor3 = OFF_COLOR
	button.Text = ""
	
	local bodyGyro, bodyVelocity
	local connection
	
	local function getRoot()
		local char = player.Character or player.CharacterAdded:Wait()
		return char:WaitForChild("HumanoidRootPart")
	end
	
	local function startFly()
		local root = getRoot()
	
		bodyGyro = Instance.new("BodyGyro")
		bodyGyro.P = 9e4
		bodyGyro.MaxTorque = Vector3.new(9e9, 9e9, 9e9)
		bodyGyro.CFrame = root.CFrame
		bodyGyro.Parent = root
	
		bodyVelocity = Instance.new("BodyVelocity")
		bodyVelocity.MaxForce = Vector3.new(9e9, 9e9, 9e9)
		bodyVelocity.Velocity = Vector3.zero
		bodyVelocity.Parent = root
	
		connection = RunService.RenderStepped:Connect(function()
			local cam = workspace.CurrentCamera
			bodyGyro.CFrame = cam.CFrame
	
			local move = Vector3.zero
			if UIS:IsKeyDown(Enum.KeyCode.W) then move += cam.CFrame.LookVector end
			if UIS:IsKeyDown(Enum.KeyCode.S) then move -= cam.CFrame.LookVector end
			if UIS:IsKeyDown(Enum.KeyCode.A) then move -= cam.CFrame.RightVector end
			if UIS:IsKeyDown(Enum.KeyCode.D) then move += cam.CFrame.RightVector end
			if UIS:IsKeyDown(Enum.KeyCode.Space) then move += cam.CFrame.UpVector end
			if UIS:IsKeyDown(Enum.KeyCode.LeftControl) then move -= cam.CFrame.UpVector end
	
			if move.Magnitude > 0 then
				bodyVelocity.Velocity = move.Unit * speed
			else
				bodyVelocity.Velocity = Vector3.zero
			end
		end)
	
		button.Text = ""
		button.BackgroundColor3 = ON_COLOR
	end
	
	local function stopFly()
		if connection then connection:Disconnect() end
		if bodyGyro then bodyGyro:Destroy() end
		if bodyVelocity then bodyVelocity:Destroy() end
	
		button.Text = ""
		button.BackgroundColor3 = OFF_COLOR
	end
	
	button.MouseButton1Click:Connect(function()
		flying = not flying
		if flying then
			startFly()
		else
			stopFly()
		end
	end)
	
	player.CharacterAdded:Connect(function()
		if flying then
			task.wait(0.2)
			startFly()
		end
	end)
	
end
coroutine.wrap(KUUVEK_fake_script)()
local function GKKGVC_fake_script() -- TextButton_5.LocalScript 
	local script = Instance.new('LocalScript', TextButton_5)

	local player = game.Players.LocalPlayer
	local mouse = player:GetMouse()
	local uis = game:GetService("UserInputService")
	local button = script.Parent
	local clickTPActive = false
	
	-- Button starts red
	button.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	
	-- Function to teleport
	local function onClick(targetPos)
		local character = player.Character
		if not character then return end
		local hrp = character:FindFirstChild("HumanoidRootPart")
		if not hrp then return end
	
		hrp.CFrame = CFrame.new(targetPos + Vector3.new(0,3,0)) -- slight offset so player doesn't get stuck in ground
	end
	
	-- Toggle click tp
	button.MouseButton1Click:Connect(function()
		clickTPActive = not clickTPActive
		if clickTPActive then
			button.BackgroundColor3 = Color3.fromRGB(0, 255, 0) -- Green
		else
			button.BackgroundColor3 = Color3.fromRGB(255, 0, 0) -- Red
		end
	end)
	
	-- Detect clicks on the world
	mouse.Button1Down:Connect(function()
		if not clickTPActive then return end
		local target = mouse.Hit
		if target then
			onClick(target.Position)
		end
	end)
	
end
coroutine.wrap(GKKGVC_fake_script)()
local function DJWOVQ_fake_script() -- TextButton_6.LocalScript 
	local script = Instance.new('LocalScript', TextButton_6)

	local player = game.Players.LocalPlayer
	local uis = game:GetService("UserInputService")
	local button = script.Parent
	local infiniteJumpActive = false
	
	-- Button starts red
	button.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	
	-- Toggle infinite jump
	button.MouseButton1Click:Connect(function()
		infiniteJumpActive = not infiniteJumpActive
		if infiniteJumpActive then
			button.BackgroundColor3 = Color3.fromRGB(0, 255, 0) -- Green
		else
			button.BackgroundColor3 = Color3.fromRGB(255, 0, 0) -- Red
		end
	end)
	
	-- Infinite jump logic
	uis.InputBegan:Connect(function(input, gameProcessed)
		if gameProcessed then return end
		if input.UserInputType == Enum.UserInputType.Keyboard and input.KeyCode == Enum.KeyCode.Space then
			if infiniteJumpActive then
				local character = player.Character
				if character and character:FindFirstChild("Humanoid") then
					character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
				end
			end
		end
	end)
	
end
coroutine.wrap(DJWOVQ_fake_script)()
local function ZFSZCR_fake_script() -- TextButton_7.LocalScript 
	local script = Instance.new('LocalScript', TextButton_7)

	local player = game.Players.LocalPlayer
	local character = player.Character or player.CharacterAdded:Wait()
	local button = script.Parent
	local runService = game:GetService("RunService")
	
	-- Button starts red
	button.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	
	local noclipActive = false
	
	-- Function to toggle noclip
	local function toggleNoclip()
		noclipActive = not noclipActive
		button.BackgroundColor3 = noclipActive and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(255, 0, 0)
	end
	
	-- Toggle on button click
	button.MouseButton1Click:Connect(toggleNoclip)
	
	-- Noclip logic every frame
	runService.Stepped:Connect(function()
		if noclipActive then
			if character and character:FindFirstChild("HumanoidRootPart") then
				for _, part in pairs(character:GetDescendants()) do
					if part:IsA("BasePart") and part.CanCollide then
						part.CanCollide = false
					end
				end
			end
		end
	end)
	
	-- Reset noclip when character respawns
	player.CharacterAdded:Connect(function(char)
		character = char
	end)
	
end
coroutine.wrap(ZFSZCR_fake_script)()
local function LBDMYLO_fake_script() -- SpeedBox_2.LocalScript 
	local script = Instance.new('LocalScript', SpeedBox_2)

	local player = game.Players.LocalPlayer
	local textBox = script.Parent
	
	local function getHumanoid()
		local char = player.Character or player.CharacterAdded:Wait()
		return char:WaitForChild("Humanoid")
	end
	
	textBox.FocusLost:Connect(function(enterPressed)
		if not enterPressed then return end
	
		local speed = tonumber(textBox.Text)
		if speed then
			getHumanoid().WalkSpeed = speed
		end
	end)
	
	-- Reapply after respawn
	player.CharacterAdded:Connect(function()
		task.wait(0.1)
		if tonumber(textBox.Text) then
			getHumanoid().WalkSpeed = tonumber(textBox.Text)
		end
	end)
	
end
