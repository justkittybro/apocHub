coroutine.wrap(KNQNE_fake_script)()
local function CAJQ_fake_script() -- TextButton_25.LocalScript 
	local script = Instance.new('LocalScript', TextButton_25)

	local HttpService = game:GetService("HttpService")
	local TeleportService = game:GetService("TeleportService")
	local Button = script.Parent
	local LP = game.Players.LocalPlayer
	
	local function getServers()
		local placeId = game.PlaceId
		local servers = {}
		local url = "https://games.roblox.com/v1/games/"..placeId.."/servers/Public?sortOrder=Asc&limit=100"
		local success, res = pcall(function()
			return game:GetService("HttpService"):GetAsync(url)
		end)
		if success then
			local data = HttpService:JSONDecode(res)
			for _, v in ipairs(data.data) do
				if v.playing < v.maxPlayers and v.id ~= game.JobId then
					table.insert(servers, v.id)
				end
			end
		end
		return servers
	end
	
	Button.MouseButton1Click:Connect(function()
		local servers = getServers()
		if #servers > 0 then
			local randomServer = servers[math.random(1, #servers)]
			TeleportService:TeleportToPlaceInstance(game.PlaceId, randomServer, LP)
		end
	end)
	
end
coroutine.wrap(CAJQ_fake_script)()
local function NYYJ_fake_script() -- TextButton_26.LocalScript 
	local script = Instance.new('LocalScript', TextButton_26)

	local Players = game:GetService("Players")
	local ReplicatedStorage = game:GetService("ReplicatedStorage")
	local RunService = game:GetService("RunService")
	local LP = Players.LocalPlayer
	
	local button = script.Parent
	local spectating = false
	local connection
	
	-- INITIAL STATE
	button.BackgroundColor3 = Color3.fromRGB(255, 0, 0) -- Red
	button.Text = ""
	
	local function GetMurderer()
		local dataFetcher = ReplicatedStorage:FindFirstChild("GetPlayerData", true)
		if dataFetcher then
			local roles = dataFetcher:InvokeServer()
			for name, data in pairs(roles) do
				if data.Role == "Murderer" then
					local target = Players:FindFirstChild(name)
					if target and target.Character and target.Character:FindFirstChild("Humanoid") then
						return target.Character.Humanoid
					end
				end
			end
		end
		return nil
	end
	
	button.MouseButton1Click:Connect(function()
		spectating = not spectating
	
		if spectating then
			button.BackgroundColor3 = Color3.fromRGB(0, 255, 0) -- Green
			button.Text = ""
	
			connection = RunService.RenderStepped:Connect(function()
				local murdererHum = GetMurderer()
	
				if murdererHum then
					workspace.CurrentCamera.CameraSubject = murdererHum
				else
					-- If murderer is not found/dead, look at self
					if LP.Character and LP.Character:FindFirstChild("Humanoid") then
						workspace.CurrentCamera.CameraSubject = LP.Character.Humanoid
					end
				end
			end)
		else
			button.BackgroundColor3 = Color3.fromRGB(255, 0, 0) -- Red
			button.Text = ""
	
			if connection then
				connection:Disconnect()
				connection = nil
			end
	
			-- Reset Camera to self
			if LP.Character and LP.Character:FindFirstChild("Humanoid") then
				workspace.CurrentCamera.CameraSubject = LP.Character.Humanoid
			end
		end
	end)
end
coroutine.wrap(NYYJ_fake_script)()
local function GFALGS_fake_script() -- TextButton_27.LocalScript 
	local script = Instance.new('LocalScript', TextButton_27)

	local Players = game:GetService("Players")
	local ReplicatedStorage = game:GetService("ReplicatedStorage")
	local RunService = game:GetService("RunService")
	local LP = Players.LocalPlayer
	
	local button = script.Parent
	local spectating = false
	local connection
	
	-- INITIAL STATE
	button.BackgroundColor3 = Color3.fromRGB(255, 0, 0) -- Red
	button.Text = ""
	
	local function GetSheriff()
		local dataFetcher = ReplicatedStorage:FindFirstChild("GetPlayerData", true)
		if dataFetcher then
			local roles = dataFetcher:InvokeServer()
			for name, data in pairs(roles) do
				-- Checks for both the original Sheriff and the Hero (whoever has the gun)
				if data.Role == "Sheriff" or data.Role == "Hero" then
					local target = Players:FindFirstChild(name)
					if target and target.Character and target.Character:FindFirstChild("Humanoid") then
						return target.Character.Humanoid
					end
				end
			end
		end
		return nil
	end
	
	button.MouseButton1Click:Connect(function()
		spectating = not spectating
	
		if spectating then
			button.BackgroundColor3 = Color3.fromRGB(0, 255, 0) -- Green
			button.Text = ""
	
			connection = RunService.RenderStepped:Connect(function()
				local sheriffHum = GetSheriff()
	
				if sheriffHum then
					workspace.CurrentCamera.CameraSubject = sheriffHum
				else
					-- Fallback to local player if Sheriff isn't found
					if LP.Character and LP.Character:FindFirstChild("Humanoid") then
						workspace.CurrentCamera.CameraSubject = LP.Character.Humanoid
					end
				end
			end)
		else
			button.BackgroundColor3 = Color3.fromRGB(255, 0, 0) -- Red
			button.Text = ""
	
			if connection then
				connection:Disconnect()
				connection = nil
			end
	
			-- Reset Camera to self
			if LP.Character and LP.Character:FindFirstChild("Humanoid") then
				workspace.CurrentCamera.CameraSubject = LP.Character.Humanoid
			end
		end
	end)
end
coroutine.wrap(GFALGS_fake_script)()
local function IHPVS_fake_script() -- TextButton_30.LocalScript 
	local script = Instance.new('LocalScript', TextButton_30)

	local button = script.Parent
	local StarterGui = game:GetService("StarterGui")
	
	button.MouseButton1Click:Connect(function()
		pcall(function()
			-- Copy to clipboard
			setclipboard('https://discord.gg/TFEEgzHjQX')
	
			-- Send Notification
			StarterGui:SetCore("SendNotification", {
				Title = "Success!",
				Text = "Discord Link Copied!",
				Duration = 5
			})
		end)
	end)
end
coroutine.wrap(IHPVS_fake_script)()
