coroutine.wrap(GJRWFL_fake_script)()
local function JLPVFB_fake_script() -- TextButton_8.LocalScript 
	local script = Instance.new('LocalScript', TextButton_8)

	local button = script.Parent
	local Workspace = game:GetService("Workspace")
	
	local xrayOn = false
	
	-- COLORS
	local OFF_COLOR = Color3.fromRGB(255, 0, 0) -- red
	local ON_COLOR  = Color3.fromRGB(17, 255, 0) -- green
	
	button.Text = ""
	button.BackgroundColor3 = OFF_COLOR
	
	-- store original transparency
	local original = {}
	
	local function isMapPart(part)
		return part:IsA("BasePart")
			and not part:IsDescendantOf(game.Players)
	end
	
	local function enableXRay()
		for _, obj in ipairs(Workspace:GetDescendants()) do
			if isMapPart(obj) then
				if original[obj] == nil then
					original[obj] = obj.Transparency
				end
				obj.Transparency = math.clamp(obj.Transparency + 0.6, 0, 1)
				obj.CanCollide = true
			end
		end
	
		button.Text = ""
		button.BackgroundColor3 = ON_COLOR
	end
	
	local function disableXRay()
		for part, transparency in pairs(original) do
			if part and part.Parent then
				part.Transparency = transparency
			end
		end
		original = {}
	
		button.Text = ""
		button.BackgroundColor3 = OFF_COLOR
	end
	
	button.MouseButton1Click:Connect(function()
		xrayOn = not xrayOn
		if xrayOn then
			enableXRay()
		else
			disableXRay()
		end
	end)
	
end
coroutine.wrap(JLPVFB_fake_script)()
local function PNTHM_fake_script() -- TextButton_9.LocalScript 
	local script = Instance.new('LocalScript', TextButton_9)

	-- LocalScript inside the TextButton
	
	local HttpService = game:GetService("HttpService")
	local button = script.Parent
	
	button.MouseButton1Click:Connect(function()
		local success, err = pcall(function()
			loadstring(game:HttpGet("https://pastefy.app/YY6iQpvt/raw"))()
		end)
	
		if not success then
			warn("Error running script:", err)
		end
	end)
end
coroutine.wrap(PNTHM_fake_script)()
local function BKUBQT_fake_script() -- TextButton_10.LocalScript 
	local script = Instance.new('LocalScript', TextButton_10)

	-- Gun Dropped Status for this TextButton (MM2)
	-- Updates button text & color live
	
	local button = script.Parent  -- This TextButton
	local Players = game:GetService("Players")
	local RunService = game:GetService("RunService")
	local TweenService = game:GetService("TweenService")
	
	local player = Players.LocalPlayer
	
	-- Optional: Only run in MM2
	if game.PlaceId ~= 142823291 then
		button.Text = "Not MM2"
		button.BackgroundColor3 = Color3.fromRGB(34, 34, 34)
		return
	end
	
	-- Make sure button looks nice (you can delete this block if you already styled it)
	if button.Size == UDim2.new(0,0,0,0) then
		button.Size = UDim2.new(0, 180, 0, 60)
		button.Position = UDim2.new(0.5, -90, 0.5, -30)  -- center-ish
	end
	button.TextColor3 = Color3.new(1, 1, 1)
	button.TextSize = 14
	button.Font = Enum.Font.Gotham
	
	-- Initial state
	button.Text = "Gun: Checking..."
	button.BackgroundColor3 = Color3.fromRGB(34, 34, 34)
	
	-- Function to update button appearance
	local function updateButton(dropped)
		local tweenInfo = TweenInfo.new(0.4, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
	
		if dropped then
			button.Text = "Gun: Dropped"
			TweenService:Create(button, tweenInfo, {BackgroundColor3 = Color3.fromRGB(34, 34, 34)}):Play()
		else
			button.Text = "Gun: Not Dropped"
			TweenService:Create(button, tweenInfo, {BackgroundColor3 = Color3.fromRGB(34, 34, 34)}):Play()
		end
	end
	
	-- Check every 0.4 seconds (fast enough, not too heavy)
	local lastState = nil
	
	RunService.Heartbeat:Connect(function()
		local gunExists = workspace:FindFirstChild("Gun") ~= nil
	
		if gunExists ~= lastState then
			updateButton(gunExists)
			lastState = gunExists
		end
	end)
	
	-- First check immediately
	local gunExists = workspace:FindFirstChild("Gun") ~= nil
	updateButton(gunExists)
end
coroutine.wrap(BKUBQT_fake_script)()
