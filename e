local function LCQVM_fake_script() -- TextButton_12.LocalScript 
	local script = Instance.new('LocalScript', TextButton_12)

	local Players = game:GetService("Players")
	local LP = Players.LocalPlayer
	local button = script.Parent
	
	-- STATE
	getgenv().AutoGrabGunEnabled = false
	
	-- INITIAL UI
	button.BackgroundColor3 = Color3.fromRGB(255, 0, 0) -- Red
	button.Text = ""
	
	button.MouseButton1Click:Connect(function()
		getgenv().AutoGrabGunEnabled = not getgenv().AutoGrabGunEnabled
	
		-- Toggle UI Appearance
		if getgenv().AutoGrabGunEnabled then
			button.BackgroundColor3 = Color3.fromRGB(0, 255, 0) -- Green
			button.Text = ""
	
			-- Start Logic
			task.spawn(function()
				while getgenv().AutoGrabGunEnabled do
					pcall(function()
						local char = LP.Character
						local root = char and char:FindFirstChild("HumanoidRootPart")
						local hum = char and char:FindFirstChild("Humanoid")
	
						if not char or not root or (hum and hum.Health <= 0) then
							return
						end
	
						-- Check if we already have the gun
						local hasGun = char:FindFirstChild("Gun") or LP.Backpack:FindFirstChild("Gun")
						if hasGun then
							return
						end
	
						-- Find the dropped gun
						-- MM2 usually names it "GunDrop", but we'll check common names too
						local droppedGun = workspace:FindFirstChild("GunDrop") or workspace:FindFirstChild("Gun") or workspace:FindFirstChild("Revolver")
	
						if droppedGun then
							local handle = droppedGun:FindFirstChild("Handle") or (droppedGun:IsA("BasePart") and droppedGun)
	
							if handle then
								local oldPos = root.CFrame -- Save location
	
								-- Instant Grab
								root.CFrame = handle.CFrame * CFrame.new(0, 1, 0)
								task.wait(0.1)
	
								-- Return to original spot
								root.CFrame = oldPos
							end
						end
					end)
					task.wait(0.1) -- Fast check
				end
			end)
		else
			button.BackgroundColor3 = Color3.fromRGB(255, 0, 0) -- Red
			button.Text = ""
		end
	end)
end
coroutine.wrap(LCQVM_fake_script)()
local function MQPKTOI_fake_script() -- TextButton_13.LocalScript 
	local script = Instance.new('LocalScript', TextButton_13)

	local RunService = game:GetService("RunService")
	local button = script.Parent
	
	local ESP_ENABLED = false
	local connection
	
	button.BackgroundColor3 = Color3.fromRGB(255, 0, 0) -- Start Red
	button.Text = ""
	
	local function CreateGunTag(part)
		if part:FindFirstChild("GunESP") then return end
	
		local bbg = Instance.new("BillboardGui")
		bbg.Name = "GunESP"
		bbg.Size = UDim2.new(0, 150, 0, 40)
		bbg.AlwaysOnTop = true
		bbg.Adornee = part
		bbg.ExtentsOffset = Vector3.new(0, 2, 0)
		bbg.Parent = part
	
		local txt = Instance.new("TextLabel")
		txt.Size = UDim2.new(1, 0, 1, 0)
		txt.BackgroundTransparency = 1
		txt.Text = "Dropped Gun"
		txt.TextColor3 = Color3.fromRGB(255, 255, 0)
		txt.TextStrokeTransparency = 0
		txt.Font = Enum.Font.SourceSansBold
		txt.TextSize = 18
		txt.Parent = bbg
	end
	
	button.MouseButton1Click:Connect(function()
		ESP_ENABLED = not ESP_ENABLED
	
		if ESP_ENABLED then
			button.BackgroundColor3 = Color3.fromRGB(0, 255, 0) -- Green
			button.Text = ""
	
			connection = RunService.RenderStepped:Connect(function()
				local gunDrop = workspace:FindFirstChild("GunDrop")
				if gunDrop then
					CreateGunTag(gunDrop)
				end
			end)
		else
			button.BackgroundColor3 = Color3.fromRGB(255, 0, 0) -- Red
			button.Text = ""
	
			if connection then
				connection:Disconnect()
				connection = nil
			end
	
			-- Cleanup tag if it exists
			local gunDrop = workspace:FindFirstChild("GunDrop")
			if gunDrop and gunDrop:FindFirstChild("GunESP") then
				gunDrop.GunESP:Destroy()
			end
		end
	end)
end
coroutine.wrap(MQPKTOI_fake_script)()
