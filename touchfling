-- Scripts:

local function CQODNPY_fake_script() -- Frame.LocalScript 
	local script = Instance.new('LocalScript', Frame)

	local Players = game:GetService("Players")
	
	local player = Players.LocalPlayer
	local button = script.Parent
	
	local enabled = false
	local touchConn
	local character
	local hrp
	
	local function setupCharacter(char)
		character = char
		hrp = char:WaitForChild("HumanoidRootPart")
	end
	
	setupCharacter(player.Character or player.CharacterAdded:Wait())
	player.CharacterAdded:Connect(setupCharacter)
	
	local function onTouched(hit)
		if not enabled then return end
		if not hrp then return end
	
		local otherChar = hit.Parent
		if not otherChar or otherChar == character then return end -- block self
	
		local humanoid = otherChar:FindFirstChildOfClass("Humanoid")
		local otherHRP = otherChar:FindFirstChild("HumanoidRootPart")
	
		if humanoid and otherHRP then
			local direction = (otherHRP.Position - hrp.Position).Unit
			otherHRP.AssemblyLinearVelocity =
				direction * 150 + Vector3.new(0, 80, 0)
			otherHRP.AssemblyAngularVelocity = Vector3.new(9999, 9999, 9999)
		end
	end
	
	button.MouseButton1Click:Connect(function()
		enabled = not enabled
		button.Text = enabled and "ON" or "OFF"
	
		if enabled and hrp then
			touchConn = hrp.Touched:Connect(onTouched)
		else
			if touchConn then
				touchConn:Disconnect()
				touchConn = nil
			end
		end
	end)
	
end
coroutine.wrap(CQODNPY_fake_script)()
local function PZAKHD_fake_script() -- Frame.LocalScript 
	local script = Instance.new('LocalScript', Frame)

	script.Parent.Active = true
	script.Parent.Draggable = true
end
coroutine.wrap(PZAKHD_fake_script)()
